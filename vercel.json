{
  "version": 2,
  "builds": [
    {
      "src": "api/image.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/image",
      "dest": "/api/image.py"
    },
    {
      "src": "/video",
      "dest": "/api/image.py"
    },
    {
      "src": "/gif", 
      "dest": "/api/image.py"
    },
    {
      "src": "/logs",
      "dest": "/api/image.py"
    },
    {
      "src": "/",
      "dest": "/api/image.py"
    }
  ]
}
